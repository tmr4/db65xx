			"type": "65816",
			"request": "launch",
			"name": "Debug file",
			"program": "${file}",
			"stopOnEntry": true,
		},
		{
			"type": "65816",
			"request": "launch",
			"name": "Lauch with args",
			"program": "${cwd}",
			"args": [
				{
					"sbin": "${cwd}\\hello_world.bin",
					"src":  "${cwd}",
					"list": "${cwd}",
